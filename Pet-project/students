  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "d51d7702-b2c4-4896-92a6-2a42c2170c71",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['avg_study_hours'] = df['attendance_rate'] * 0.01 * df['study_hours_per_week']\n",
    "df['avg_study_hours_group'] = pd.qcut(df['avg_study_hours'], 3, labels=False)\n"
   ]
  },
